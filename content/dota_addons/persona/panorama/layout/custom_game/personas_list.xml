<root>
  <styles>
    <include src="file://{resources}/styles/custom_game/personas_list.css" />
  </styles>
  <scripts>
    <include src="file://{resources}/scripts/custom_game/personas_list.js" /> 
  </scripts>
	<Panel hittest="false" class="PersonaTooltipRoot">
    <!-- <Image id="PersonaImage"  src="file://{images}/custom_game/slime.png"/> -->
    <!-- <DOTAItemImage id="PersonaImage"  src="file://{images}/custom_game/slime.png"/> -->
    <!-- <DOTAItemImage id="PersonaImage"/> -->
		<Panel hittest="false" id="PersonaAttributesContainer" class="PersonaAttributesContainerInvisible">
      <Panel hittest="false" class="PersonaAttributeBox">
        <Label hittest="false"  class="PersonaAttribute" id="PersonaStrength"/>
      </Panel>
      <Panel hittest="false" class="PersonaAttributeBox">
        <Label hittest="false"  class="PersonaAttribute" id="PersonaMagic"/>
      </Panel>
      <Panel hittest="false" class="PersonaAttributeBox">
        <Label hittest="false"  class="PersonaAttribute" id="PersonaEndurance"/>
      </Panel>
      <Panel hittest="false" class="PersonaAttributeBox">
        <Label hittest="false"  class="PersonaAttribute" id="PersonaSwiftness"/>
      </Panel>
      <Panel hittest="false" class="PersonaAttributeBox">
        <Label hittest="false"  class="PersonaAttribute" id="PersonaAgility"/>
      </Panel>
    </Panel>
    <Panel hittest="false" class="PersonaMouseover" onmouseover="RemoveStyle(PersonaAttributesContainer, PersonaAttributesContainerInvisible)" onmouseout="AddStyle(PersonaAttributesContainer, PersonaAttributesContainerInvisible)">
      <Image id="PersonaImage"  src="file://{images}/custom_game/slime.png"/>
    </Panel>
	</Panel>
</root>
